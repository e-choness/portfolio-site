{
    "title": "Portfolio-website",
    "name": "portfolio-site",
    "version": "1.0.2",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js",
        "deploy": "gh-pages -d dist"
    },
    "description": "Resume is a simple, yet elegant resume and CV theme for Bootstrap 4.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive"
    ],
    "homepage": "https://e-choness.github.io/portfolio-site",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/e-choness/portfolio-site"
    },
    "dependencies": {
        "bootstrap": "5.2.3",
        "devicon": "^2.15.1"
    },
    "devDependencies": {
        "autoprefixer": "10.4.14",
        "browser-sync": "^3.0.4",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "gh-pages": "^6.3.0",
        "postcss": "^8.5.3",
        "prettier": "2.8.6",
        "pug": "^3.0.3",
        "sass": "1.60.0",
        "shelljs": "^0.10.0",
        "upath": "2.0.1"
    }
}
